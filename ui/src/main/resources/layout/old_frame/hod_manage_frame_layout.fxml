<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXTextField?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.godavari.appsnest.fms.ui.controller.HodManageFrameController">
   <children>
      <HBox prefHeight="400.0" prefWidth="600.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <VBox prefHeight="200.0" prefWidth="100.0">
               <children>
                  <fx:include source="../list/saved_hod_manage_list_layout.fxml" />
               </children></VBox>
            <VBox prefHeight="200.0" prefWidth="100.0">
               <children>
                  <JFXTextField fx:id="jfxTfDepartmentName" labelFloat="true" promptText="%department_name" />
                  <fx:include source="../combobox/saved_hod_combo_box_layout.fxml" />
                  <ButtonBar prefHeight="40.0" prefWidth="200.0">
                     <buttons>
                        <JFXButton fx:id="jfxBReset" buttonType="RAISED" text="%reset_department" />
                        <JFXButton fx:id="jfxBInsert" buttonType="RAISED" text="%insert_department" />
                        <JFXButton fx:id="jfxBDelete" buttonType="RAISED" text="%delete_department" />
                        <JFXButton fx:id="jfxBUpdate" buttonType="RAISED" text="%update_department" />
                     </buttons>
                  </ButtonBar>
               </children>
            </VBox>
         </children>
      </HBox>
   </children>
</AnchorPane>
