<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXDatePicker?>
<?import com.jfoenix.controls.JFXTextField?>
<?import com.jfoenix.controls.JFXTimePicker?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.godavari.appsnest.fms.ui.controller.AccountFrameController">
    <children>
        <HBox prefHeight="400.0" prefWidth="600.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <children>
            <VBox prefHeight="200.0" prefWidth="100.0">
               <children>
                  <fx:include source="../list/saved_account_list_layout.fxml" />
               </children>
            </VBox>
                <VBox prefHeight="400.0" prefWidth="611.0">
                    <children>
                        <JFXDatePicker fx:id="jfxDatePicker" promptText="%select_date" />
                        <JFXTimePicker fx:id="jfxTimePicker" promptText="%select_time" />
                        <fx:include source="../combobox/saved_hod_manage_combo_box_layout.fxml" />
                        <fx:include source="../combobox/saved_vehicle_assigned_combo_box_layout.fxml" />
                        <JFXTextField fx:id="jfxTFInput" labelFloat="true" promptText="%input_value" />
                        <JFXTextField fx:id="jfxTFOutput" labelFloat="true" promptText="%output_value" />
                        <JFXTextField fx:id="jfxTFCurrentReading" labelFloat="true" promptText="%current_reading_value" />
                        <JFXTextField fx:id="jfxTFOwner" labelFloat="true" promptText="%owner_value" />
                        <ButtonBar prefHeight="40.0" prefWidth="200.0">
                            <buttons>
                                <JFXButton fx:id="jfxBReset" buttonType="RAISED" text="%reset_account" />
                                <JFXButton fx:id="jfxBInsert" buttonType="RAISED" text="%insert_account" />
                                <JFXButton fx:id="jfxBDelete" buttonType="RAISED" text="%delete_account" />
                                <JFXButton fx:id="jfxBUpdate" buttonType="RAISED" text="%update_account" />
                            </buttons>
                        </ButtonBar>
                    </children>
                </VBox>
            </children>
        </HBox>
    </children>
</AnchorPane>
